{
  "expo": {
    "name": "PrestaFacil",
    "slug": "prestafacil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/prestafacil-icon.jpg",
    "scheme": "prestafacil",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jhonny.prestafacil",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "La app necesita acceder a la c치mara para tomar fotos de los objetos.",
        "NSPhotoLibraryUsageDescription": "La app necesita acceder a tus fotos para subir im치genes de los objetos."
      }
    },
    "android": {
      "package": "com.jhonny.prestafacil",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/prestafacil-icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/prestafacil-icon.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "La app necesita acceder a tus fotos para subir im치genes de los objetos.",
          "cameraPermission": "La app necesita acceder a la c치mara para tomar fotos de los objetos."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e788a7c8-a678-4e20-adbf-0f411228e989"
      }
    }
  }
}